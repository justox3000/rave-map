<%= render "shared/navbar" %>
<%= render "shared/search_form" %>
<div class="dashboard-container">
  <h1>Your Events</h1>
  <div class="Calendar">
    <%= month_calendar(events: @parties) do |date, parties| %>
      <%= date %>
      <% parties.each do |party| %>
        <div class="party">
          <p><%= party.title %></p>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
